<% @tasks.each do |task| %>
  <h2><%= task.name %></h2>
  <p><%= task.content %></p>
  <p><%= link_to "show", task_path(task) %></p>
  <p><%= link_to "edit", edit_task_path(task) %></p>
  <p><%= link_to "Destroy", task_path(task),
      method: :delete,
      data: { confirm: "Are you sure?" }%></p>





<% end %>
